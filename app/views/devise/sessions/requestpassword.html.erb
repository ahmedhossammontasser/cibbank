<h2>CIB Bank | Log in</h2>



<%= form_for(resource, as: resource_name, url: sendrequestpassword_path(resource_name)) do |f| %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "Send Password" %>
  </div>


<% end %>



<%= render "devise/shared/links" %>